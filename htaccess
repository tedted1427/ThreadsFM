

# ThreadsFM pretty URLs
RewriteEngine On

# /register verwijst naar register.php
RewriteRule ^register$ register.php [L]

# /login verwijst naar login.php
RewriteRule ^login$ login.php [L]

# /logout verwijst naar logout.php
RewriteRule ^logout$ logout.php [L]

# /vote verwijst naar vote.php
RewriteRule ^vote$ vote.php [L]

# /home verwijst naar index.php
RewriteRule ^home$ index.php [L]

# /account verwijst naar account.php
RewriteRule ^account$ account.php [L]

